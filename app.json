{
  "expo": {
    "name": "SwiggyRestaurant App",
    "slug": "swiggyrestaurant-app",
    "description":  "This is my first attempt at react native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/hicon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/spla.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "assetBundlePatterns": [
      "**/*"
